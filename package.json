{
  "name": "vscode-open-folder-new-tab",
  "displayName": "Open Folder in New Browser Tab",
  "description": "Open folders in new browser tabs in VS Code Server",
  "repository": "https://github.com/SongDrop/vscode_server_open_new_tab",
  "publisher": "songdropltd",
  "icon": "media/logo.png",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-open-folder-new-tab.openFolderNewTab"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-open-folder-new-tab.openFolderNewTab",
        "title": "Open in New Browser Tab",
        "category": "Folder"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-open-folder-new-tab.openFolderNewTab",
          "group": "navigation",
          "when": "explorerResourceIsFolder"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.17.0",
    "typescript": "^5.7.2"
  }
}
